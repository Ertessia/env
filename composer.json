{
    "name": "ertessia/env",
    "description": "A simple package used to mediate between environment variables and PHP.",
    "keywords": [
        "Ertessia",
        "env",
        "environment variables"
    ],
    "homepage": "https://github.com/Ertessia/env",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens de Nies",
            "email": "jens@ertessia.com"
        },
        {
            "name": "Jeroen van den Bos",
            "email": "jeroen@ertessia.com"
        }
    ],
    "require": {
        "php": "~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5"
    },
    "autoload": {
        "psr-4": {
            "Ertessia\\Env\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ertessia\\Env\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}